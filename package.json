{
  "name": "test-jest",
  "version": "1.0.0",
  "description": "Example project to learn how to use jest with typescript and express",
  "main": "dist/main.js",
  "private": "true",
  "scripts": {
    "compile": "npx tsc",
    "start": "npm run compile && node ./dist/main.js",
    "start-dev": "node ./node_modules/nodemon/bin/nodemon.js -e ts --exec \"npm run start\"",
    "lint": "npx tslint --project tsconfig.json --config tslint.json",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "typescript",
    "express",
    "jest",
    "tslint"
  ],
  "author": "yannnick.baudraz@cpnv.ch",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.4",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.20",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.14.33",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": ">=0.12",
    "npm": ">=6.14.4"
  }
}
